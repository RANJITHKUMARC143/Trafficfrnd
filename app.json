{
  "expo": {
    "name": "Traffic Friend",
    "slug": "traffic-frnd-new",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "trafficfriend",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trafficfriend.app",
      "config": {
        "googleMapsApiKey": "AIzaSyAJX6Yjwz2DiEHjWE_0HIBvEkiYpS5TXAU"
      }
    },
    "android": {
      "package": "com.trafficfriend.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAJX6Yjwz2DiEHjWE_0HIBvEkiYpS5TXAU"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Traffic Friend to use your location to find nearby delivery buddies and improve delivery accuracy."
        }
      ],
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d55d3fc-66c4-4fce-8c61-602ef7e45516"
      }
    }
  }
}
